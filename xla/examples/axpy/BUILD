# The analyzer binary needs access to a couple of PJRT proto headers that
# are not pulled in by the existing dependencies above.  The build failure
# during compilation showed the missing headers:
#   bazel-out/.../xla/pjrt/proto/pjrt_partial_program.pb.h
#   bazel-out/.../xla/pjrt/proto/topology_description.pb.h
#
# Those headers are generated by the corresponding proto cc libraries, so
# we add explicit dependencies here.  Without them the analyzer target will
# fail with "undeclared inclusion(s)" as seen by the user.
cc_binary(
    name = "priority_fusion_analyzer",
    srcs = ["priority_fusion_analyzer.cc"],
    deps = [
        ":compiler_utils",
        # core utilities used by the analyzer executable.
        "//xla:error_spec",
        "//xla:literal",
        "//xla:literal_util",
        "//xla:debug_options_flags",

        # we are exercising the PriorityFusion pass itself, so depend on the
        # cc_library that actually compiles the pass.  the original target in
        # this file accidentally referenced the directory rather than the
        # specific library which does not exist and caused the build to fail.
        "//xla/backends/gpu/transforms:priority_fusion",

        # headers pulled in by the analyzer source file.
        "//xla/hlo/analysis:alias_info",
        "//xla/hlo/analysis:symbolic_map",        # contains symbolic_expr.h
        "//xla/hlo/ir:hlo",                      # core HLO IR types
        "//xla/hlo/parser:hlo_parser",

        # other pieces of the GPU stack that the analyzer touches.
        "//xla/service/gpu:backend_configs_cc",
        "//xla/service/gpu:gpu_device_info_for_tests",
        "//xla/service/gpu:gpu_fusible",
        "//xla/service/gpu:hlo_fusion_analysis",
        "//xla/service/gpu/model:gpu_hlo_cost_analysis",
        "//xla/service:dump",

        # PJRT client libraries provide almost every header under xla/pjrt/
        # that the analyzer source includes (and more).  In the original
        # build failure only the proto headers were pulled in; adding these
        # libraries makes the target behave similarly to the existing
        # examples such as stablehlo_compiler and stablehlo_gpu_compiler.
        "//xla/pjrt:pjrt_api",
        "//xla/pjrt:pjrt_client",
        "//xla/pjrt:pjrt_executable",
        "//xla/pjrt/c:pjrt_c_api_cpu",
        "//xla/pjrt/c:pjrt_c_api_hdrs",
        "//xla/pjrt/c_api_client:pjrt_c_api_client",
        "//xla/pjrt/plugin/xla_gpu:xla_gpu_client_options",
        "//xla/pjrt/plugin/xla_gpu:xla_gpu_pjrt_client",
        "//xla/pjrt:pjrt_stream_executor_client",

        # pull in the umbrella service libraries to satisfy the many
        # "xla/service/..." headers referenced by the PJRT and client
        # implementation.
        "//xla/service:service",
        "//xla/service:local_service",

        # the stream executor address/memory allocators are pulled in by the
        # stream executor components used by PJRT; include them explicitly
        # to avoid undeclared inclusion errors.
        "//xla/stream_executor:stream_executor_memory_allocator",
        "//xla/stream_executor:stream_executor_address_allocator",

        # pull in the PJRT proto cc libraries so that the generated headers
        # are available at compile time.  these are the missing deps from the
        # earlier build error.
        "//xla/pjrt/proto:pjrt_partial_program_proto_cc",
        "//xla/pjrt/proto:topology_description_proto_cc",

        # retain some of the previously added deps; they are mostly harmless
        # and pull in common utilities used throughout XLA.
        "//xla/hlo/translate:stablehlo",
        "//xla/stream_executor:cuda_platform",
        "//xla/pjrt/plugin/xla_gpu:gpu_static_registration",
        "//xla/stream_executor:device_description",
        "//xla/tsl/platform:env",
        "//xla/tsl/platform:errors",
        "//xla/tsl/platform:statusor",
        "@com_google_absl//absl/log:check",
        "@com_google_absl//absl/status:statusor",
        "@com_google_absl//absl/strings:string_view",
        "@llvm-project//mlir:FuncDialect",
        "@llvm-project//mlir:IR",
        "@llvm-project//mlir:Parser",
        "@stablehlo//:register",
        "@tsl//tsl/platform:path",
    ],
)


cc_binary(
    name = "stablehlo_compiler",
    srcs = ["stablehlo_compiler.cc"],
    deps = [
        "//xla:error_spec",
        "//xla:literal",
        "//xla:literal_util",
        "//xla/pjrt:pjrt_api",
        "//xla/pjrt:pjrt_client",
